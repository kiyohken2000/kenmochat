{
  "expo": {
    "name": "PINE",
    "slug": "pine",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.1.5",
    "orientation": "portrait",
    "icon": "./assets/images/logo-sm.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 10000
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "net.votepurchase.pine",
      "buildNumber": "1.1.6",
      "infoPlist": {
        "NSCameraUsageDescription": "QRコードで連絡先を追加するときにカメラを使用します",
        "NSPhotoLibraryUsageDescription": "プロフィールアバターの変更及び画像の投稿のためフォトライブラリを使用します"
      },
      "userInterfaceStyle": "automatic"
    },
    "android": {
      "package": "net.votepurchase.pine",
      "versionCode": 18,
      "googleServicesFile": "./google-services.json",
      "userInterfaceStyle": "automatic",
      "permissions": [
        "VIBRATE",
        "CAMERA",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "js",
        "jsx",
        "svg",
        "ts",
        "tsx"
      ]
    },
    "description": ""
  }
}
