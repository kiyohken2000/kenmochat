{
  "expo": {
    "name": "PINE",
    "slug": "pine",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/logo-sm.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 10000
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "net.votepurchase.pine",
      "buildNumber": "1.0.2",
      "infoPlist": {
        "NSCameraUsageDescription": "Use the camera to upload images.",
        "NSPhotoLibraryUsageDescription": "Use the photo library to upload images."
      }
    },
    "android": {
      "package": "net.votepurchase.pine",
      "versionCode": 2,
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "VIBRATE"
      ]
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "js",
        "jsx",
        "svg",
        "ts",
        "tsx"
      ]
    },
    "description": ""
  }
}
